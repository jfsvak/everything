{
	"name": "everythingreact",
	"version": "1.0.0",
	"description": "React Client for EVErything",
	"author": "Jesper Svak",
	"license": "ISC",
	"scripts": {
		"build:dev": "webpack-dev-server --config webpack.config.dev.js --host 0.0.0.0",
		"build:production": "webpack --config webpack.config.prod.js",
		"build:redux-test": "webpack --config webpack.config.redux-test.js && node redux-test-dist/app.js",
		"build:offline": "webpack --config webpack.config.offline.js",
		"clean": "rimraf ./dist",
		"start": "npm run build:dev",
		"redux-test": "npm run build:redux-test",
		"build": "npm run clean && npm run build:production",
		"offline": "npm run clean && npm run build:offline",
		"test": "jest"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"bootstrap": "^4.1.1",
		"chart.js": "^2.7.2",
		"immutable": "^3.8.2",
		"moment": "^2.22.1",
		"prop-types": "^15.6.1",
		"react": "^16.3.2",
		"react-chartjs-2": "^2.7.2",
		"react-dom": "^16.3.2",
		"react-loadable": "^5.4.0",
		"react-rangeslider": "^2.2.0",
		"react-redux": "^5.0.7",
		"react-router": "^4.2.0",
		"react-router-dom": "^4.2.2",
		"redux": "^4.0.0",
		"redux-observable": "^1.0.0",
		"redux-thunk": "^2.2.0",
		"rxjs": "^6.2.2"
	},
	"devDependencies": {
		"@types/axios": "^0.14.0",
		"@types/jquery": "^3.3.6",
		"@types/node": "^10.0.4",
		"@types/prop-types": "^15.5.2",
		"@types/react": "^16.3.13",
		"@types/react-dom": "^16.0.5",
		"@types/react-router": "^4.0.24",
		"@types/redux-immutable-state-invariant": "^2.0.4",
		"@types/systemjs": "^0.20.6",
		"babel-polyfill": "^6.26.0",
		"copy-webpack-plugin": "^4.5.1",
		"css-loader": "^0.28.10",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.2.0",
		"jest": "^23.5.0",
		"less": "^3.0.4",
		"less-loader": "^4.1.0",
		"npm-sass": "^2.3.0",
		"raw-loader": "^0.5.1",
		"redux-devtools-extension": "^2.13.5",
		"redux-immutable-state-invariant": "^2.1.0",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.21.0",
		"sw-precache-webpack-plugin": "^0.11.5",
		"ts-jest": "^23.1.4",
		"ts-loader": "^4.3.0",
		"tslib": "^1.9.0",
		"typescript": "^2.8.3",
		"webpack": "^4.8.1",
		"webpack-cli": "^2.1.3",
		"webpack-dev-server": "^3.1.4"
	},
	"jest": {
		"transform": {
			"^.+\\.(t|j)sx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
